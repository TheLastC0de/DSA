import java.util.Scanner; 
class Node ( 
int data; 
Node next; 
Node (int data) { 
this.data = data; this.next = null; 
} 
} 
public class SinglyLinkedList ( 
Node head; 
public void insert (int data) { 
Node newNode = new Node (data); if (head == null) { 
head = newNode; 
} else { 
Node temp = head; 
while (temp.next != null) { 
temp temp.next; 
temp.next = newNode; 
} 
} 
} 
public void display () { 
Node temp = head; 
while (temp != null) { 
System.out.print (temp.data + "); temp = temp.next; 
} 
public static void main(String[] args) { Scanner sc = new Scanner(System.in); SinglyLinkedList list = new 
SinglyLinkedList(); 
System.out.print ("Enter number of nodes: "); int n = sc.nextInt(); 
for (int i = 0; i < n; i++) { 
System.out.print ("Enter data for node " + 
(i + 1) + ": "); 
} 
int data = sc.nextInt(); 
list.insert(data); 
System.out.println("Linked List elements: "); 
list.display(); 
sc.close(); 
} 
